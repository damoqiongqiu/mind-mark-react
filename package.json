  "keywords": [
    "react",
    "react-router",
    "mindmark"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "start": "npm run start:dev-backend",
    "start:dev-mock": "cross-env NODE_ENV=development DATA_SOURCE=mock NODE_OPTIONS=--max_old_space_size=4096 webpack serve",
    "start:dev-backend": "cross-env NODE_ENV=development DATA_SOURCE=backend NODE_OPTIONS=--max_old_space_size=4096 webpack serve",
    "start:prod-mock": "cross-env NODE_ENV=production DATA_SOURCE=mock NODE_OPTIONS=--max_old_space_size=4096 webpack serve",
    "start:prod-backend": "cross-env NODE_ENV=production DATA_SOURCE=backend NODE_OPTIONS=--max_old_space_size=4096 webpack serve",
    "start-analyzer": "cross-env NODE_ENV=development ANALYZE=1 DATA_SOURCE=mock NODE_OPTIONS=--max_old_space_size=4096 webpack serve",
    "start-unused": "cross-env NODE_ENV=development UNUSED=1 DATA_SOURCE=mock NODE_OPTIONS=--max_old_space_size=4096 webpack serve",
    "start-smp": "cross-env NODE_ENV=development SMP=1 ATA_SOURCE=mock NODE_OPTIONS=--max_old_space_size=4096 webpack serve",
    "build": "cross-env NODE_ENV=production DATA_SOURCE=backend NODE_OPTIONS=--max_old_space_size=4096 webpack",
    "build-analyzer": "cross-env NODE_ENV=production ANALYZE=1  DATA_SOURCE=mock NODE_OPTIONS=--max_old_space_size=4096 webpack",
    "build-smp": "cross-env NODE_ENV=production SMP=1 DATA_SOURCE=mock NODE_OPTIONS=--max_old_space_size=4096 webpack",
    "generateAnalyzerFile": "cross-env NODE_ENV=production NODE_OPTIONS=--max_old_space_size=4096 webpack --profile --json > stats.json",
    "analyzer": "cross-env NODE_OPTIONS=--max_old_space_size=4096 webpack-bundle-analyzer --port 8888 ./docs/stats.json"
  },
